{
    "openapi": "3.0.3",
    "info": {
        "title": "API Tray",
        "description": "API Tray",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://localhost:8000"
        }
    ],
    "paths": {
        "/api/sellers": {
            "get": {
                "description": "List sellers",
                "responses": {
                    "200": {
                        "description": "List all sellers",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ListSellers"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ListSellerInternalServerError"
                                }
                            }
                        }
                    },
                    "503": {
                        "description": "Service Unavaiable",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ListSellerServiceUnavaiable"
                                }
                            }
                        }
                    }
                }
            },
            "post": {
                "summary": "Create a Seller",
                "description": "Create a seller",
                "operationId": "createSeller",
                "responses": {
                    "201": {
                        "description": "Created response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/CreatedSeller"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Data Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/CreateSellerDataError"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/CreateSellerValidationError"
                                }
                            }
                        }
                    }
                },
                "requestBody": {
                    "description": "Create a single seller",
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/CreateSeller"
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "SellerEntity": {
                "properties": {
                    "id": {
                        "type": "integer",
                        "description": "Seller ID",
                        "default": "Seller ID"
                    },
                    "name": {
                        "type": "string",
                        "description": "Seller name",
                        "default": "Seller name"
                    },
                    "email": {
                        "type": "string",
                        "description": "Seller email",
                        "default": "Seller email"
                    },
                    "commission": {
                        "type": "number",
                        "description": "Seller commission",
                        "default": 8.5
                    }
                }
            },
            "ListSellerInternalServerError": {
                "type": "object",
                "properties": {
                    "success": {
                        "type": "boolean",
                        "default": false
                    },
                    "data": {
                        "type": "string",
                        "default": "Some Worng Happens"
                    }
                }
            },
            "ListSellerServiceUnavaiable": {
                "type": "object",
                "properties": {
                    "success": {
                        "type": "boolean",
                        "default": false
                    },
                    "data": {
                        "type": "string",
                        "default": "Service Unavaiable"
                    }
                }
            },
            "ListSellers": {
                "type": "object",
                "properties": {
                    "success": {
                        "type": "boolean"
                    },
                    "data": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/SellerEntity"
                        }
                    }
                }
            },
            "CreateSellerValidationError": {
                "type": "object",
                "properties": {
                    "success": {
                        "type": "boolean",
                        "default": false
                    },
                    "data": {
                        "type": "object",
                        "properties": {
                            "name": {
                                "type": "string",
                                "default": "This field cannot be empty"
                            },
                            "email": {
                                "type": "string",
                                "default": "This cannot be empty and must be an email"
                            }
                        }
                    }
                }
            },
            "CreateSellerDataError": {
                "type": "object",
                "properties": {
                    "success": {
                        "type": "boolean",
                        "default": false
                    },
                    "data": {
                        "type": "string",
                        "default": "Some Worng Happens"
                    }
                }
            },
            "CreatedSeller": {
                "type": "object",
                "properties": {
                    "success": {
                        "type": "boolean",
                        "default": true
                    },
                    "data": {
                        "type": "object",
                        "properties": {
                            "id": {
                                "type": "integer",
                                "default": 1
                            },
                            "name": {
                                "type": "string",
                                "default": "Seller name"
                            },
                            "email": {
                                "type": "string",
                                "default": "Seller email"
                            },
                            "commission": {
                                "type": "number",
                                "default": 8.5
                            }
                        }
                    }
                }
            },
            "CreateSeller": {
                "type": "object",
                "required": [
                    "name",
                    "email"
                ],
                "properties": {
                    "name": {
                        "type": "string",
                        "description": "Seller name",
                        "default": "Seller name"
                    },
                    "email": {
                        "type": "string",
                        "description": "Seller email",
                        "default": "Seller email"
                    },
                    "commission": {
                        "type": "number",
                        "description": "Seller commission",
                        "default": 8.5
                    }
                }
            }
        }
    }
}
